function(collect_asm_compiler)
	if (NOT "${CMAKE_ASM_ATT_COMPILER}" STREQUAL "")
		set(ASM_COMPILER "ASM-ATT" PARENT_SCOPE)
		set(ASM_COMPILER_PRESENT true PARENT_SCOPE)
		message("ASM Compiler: ${CMAKE_ASM_ATT_COMPILER}")
	endif()
	if (NOT "${CMAKE_ASM_COMPILER}" STREQUAL "")
		set(ASM_COMPILER "ASM" PARENT_SCOPE)
		set(ASM_COMPILER_PRESENT true PARENT_SCOPE)
		message("ASM Compiler: ${CMAKE_ASM_COMPILER}")
	endif()
	if (NOT "${CMAKE_ASM_NASM_COMPILER}" STREQUAL "")
		set(ASM_COMPILER "ASM_NASM" PARENT_SCOPE)
		set(ASM_COMPILER_PRESENT true PARENT_SCOPE)
		message("ASM Compiler: ${CMAKE_ASM_NASM_COMPILER}")
	endif()
	if (NOT "${CMAKE_ASM_MASM_COMPILER}" STREQUAL "")
		set(ASM_COMPILER "ASM_MASM" PARENT_SCOPE)
		set(ASM_COMPILER_PRESENT true PARENT_SCOPE)
		message("ASM Compiler: ${CMAKE_ASM_MASM_COMPILER}")
	endif()
	if (NOT "${CMAKE_ASM_MARMASM_COMPILER}" STREQUAL "")
		set(ASM_COMPILER "ASM_MARMASM" PARENT_SCOPE)
		set(ASM_COMPILER_PRESENT true PARENT_SCOPE)
		message("ASM Compiler: ${CMAKE_ASM_MARMASM_COMPILER}")
	endif()	
endfunction()